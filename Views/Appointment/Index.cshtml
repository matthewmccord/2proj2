
@model List<_2proj2.Models.User>

<table class="table">
    <thead>
        <tr>
            <td>Date</td>
            <td>Time</td>
            <td>Name</td>
            <td>Email</td>
            <td>Phone</td>
            <td>Group Size</td>
            <td>Actions</td>
        </tr>
    </thead>
    <tbody>
        @foreach (User u in Model)
        {
            <tr>
                <td>@u.Start.Date.ToString("MM/dd/yyyy")</td>
                <td>@u.Start.ToString("hh:mm tt")</td>
                <td>@u.Name</td>
                <td>@u.Email</td>
                <td>@u.Phone</td>
                <td>@u.GroupSize</td>
                <td>
                    <a href="/Appointments/Update/@u.GroupId" class="btn btn-info">Edit</a>
                    <a href="/Appointments/Delete/@u.GroupId" class="btn btn-danger">Delete</a>
                </td>
            </tr>
        }

    </tbody>
</table>
